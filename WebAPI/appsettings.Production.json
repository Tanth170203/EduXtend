{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_PRODUCTION_SERVER;Database=EduXtend;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=true;Encrypt=true;"
  },
  "GoogleAuth": {
    "ClientIds": ["YOUR_GOOGLE_CLIENT_ID"],
    "HostedDomain": "fpt.edu.vn"
  },
  "Import": {
    "AllowedEmailDomain": "fpt.edu.vn",
    "BatchSize": 200
  },
  "Jwt": {
    "Issuer": "EduXtend",
    "Audience": "EduXtend.Client",
    "Key": "YOUR_SUPER_SECRET_JWT_KEY_AT_LEAST_32_CHARACTERS_LONG",
    "AccessTokenMinutes": 60,
    "RefreshTokenDays": 30
  }
}

